{
	"name" : "wabel/threads-io-php-plug",
	"description" : "Plug Threads.io to your PHP application and start collecting events data from your users.",
	"type" : "library",
	"homepage" : "http://wwww.wabel.com",
	"keywords" : [
	  	"Threads.io",
		"Wrapper",
		"PHP"
	],
	"require" : {
		"php" : ">=5.4",
		"guzzlehttp/guzzle": "~5.0",
        "symfony/http-foundation": "~2.1",
        "psr/log": "~1.0"
	},
    "require-dev" : {
	  "phpunit/phpunit": "~4.0",
	  "satooshi/php-coveralls": "dev-master"
	},
	"autoload" : {
		"psr-4" : {
			"Wabel\\Threads.io\\" : "src"
		}
	},
    "autoload-dev" : {
  	    "psr-4" : {
          "Wabel\\Threads.io\\" : "tests"
	    }
    }
}
